{
  "name": "agentic-shareables-middleware",
  "version": "1.0.0",
  "description": "Serverless middleware for shareable tokens communication",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p typecheck:watch start:dev",
    "typecheck:watch": "tsc --noEmit --watch",
    "start:dev": "nodemon --exec tsx src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc && npm run copy:worker",
    "copy:worker": "node scripts/copy-worker.js || powershell -Command \"Copy-Item src/services/slack-worker.js dist/services/slack-worker.js -Force\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean": "rm -rf .esbuild .serverless dist",
    "offline": "serverless offline",
    "deploy": "npm run clean && serverless deploy",
    "deploy:dev": "npm run clean && serverless deploy --stage dev",
    "deploy:prod": "npm run clean && serverless deploy --stage prod"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.922.0",
    "@aws-sdk/client-dynamodb": "^3.927.0",
    "@aws-sdk/lib-dynamodb": "^3.927.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.13.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.11",
    "serverless-offline": "^13.3.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/aws-lambda": "^8.10.157",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^4.0.7",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.55.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.7"
  }
}
